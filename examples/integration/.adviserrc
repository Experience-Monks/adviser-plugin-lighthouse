{
  "plugins": [
    "lighthouse"
  ],
  "rules": {
    "lighthouse/scores": [
      "error",
      {
        "best-practices": 1,
        "pwa": 1,
        "seo": 1,
        "accessibility": 1,
        "performance": 1
      }
    ],
    "lighthouse/audits": [
      "error",
      {
        "mainthread-work-breakdown": 1,
        "image-aspect-ratio": 1,
        "not-a-valid-audit": 1
      }
    ]
  },
  "settings": {
    "lighthouse": {
      "url": "https://google.com",
      "configPath": "dev.lh.config.json"
    }
  }
}
