{
  "plugins": ["lighthouse"],
  "lighthouse": {
    "url": "https://google.com",
  },
  "rules": {
    "lighthouse/performance": ["error", {"score": 0.9}],
    "lighthouse/accessibility": ["error", {"score": 0.9}],
    "lighthouse/seo": ["error", {"score": 0.9}],
    "lighthouse/best-practices": ["error", {"score": 0.9}],
    "lighthouse/pwa": ["error", {"score": 0.5}]
  }
}
